{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/house/index",
      "style": {
        "navigationBarTitleText": "首页",
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/price/index",
      "style": {
        "navigationBarTitleText": "工种",
        "navigationStyle": "custom",
        "disableScroll": true
      }
    },
    {
      "path": "pages/materials/index",
      "style": {
        "navigationBarTitleText": "辅材商城"
      }
    },
    {
      "path": "pages/mine/index",
      "style": {
        "navigationBarTitleText": "我的",
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "装修助手",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#fff"
  },
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#00cec9",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/house/index",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/house_selected.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/price/index",
        "iconPath": "static/tabbar/price.png",
        "selectedIconPath": "static/tabbar/price_selected.png",
        "text": "工价"
      },
      {
        "pagePath": "pages/materials/index",
        "iconPath": "static/tabbar/materials.png",
        "selectedIconPath": "static/tabbar/materials_selected.png",
        "text": "辅材"
      },
      {
        "pagePath": "pages/mine/index",
        "iconPath": "static/tabbar/profile.png",
        "selectedIconPath": "static/tabbar/profile_selected.png",
        "text": "我的"
      }
    ]
  },
  "subPackages": [
    {
      "root": "subpackages",
      "pages": [
        {
          "path": "login/index",
          "style": {
            "navigationBarTitleText": "登录",
            "disableScroll": true,
            "navigationStyle": "custom"
          }
        },
        {
          "path": "city-list/index",
          "style": {
            "navigationBarTitleText": "城市选择",
            "disableScroll": true
          }
        },
        {
          "path": "product-detail/index",
          "style": {
            "navigationBarTitleText": "商品详情",
            "disableScroll": true
          }
        },
        {
          "path": "search-product/index",
          "style": {
            "navigationBarTitleText": "搜索商品",
            "disableScroll": true
          }
        },
        {
          "path": "case-item-detail/index",
          "style": {
            "navigationBarTitleText": "案例详情",
            "disableScroll": true
          }
        },
        {
          "path": "work-type-detail/index",
          "style": {
            "navigationBarTitleText": "工种详情",
            "disableScroll": true
          }
        },
        {
          "path": "profile-edit/index",
          "style": {
            "navigationBarTitleText": "编辑个人信息",
            "disableScroll": true
          }
        },
        {
          "path": "orders/index",
          "style": {
            "navigationBarTitleText": "我的订单",
            "disableScroll": true
          }
        },
        {
          "path": "order-detail/index",
          "style": {
            "navigationBarTitleText": "订单详情",
            "disableScroll": true
          }
        },
        {
          "path": "address/index",
          "style": {
            "navigationBarTitleText": "地址管理",
            "disableScroll": true
          }
        },
        {
          "path": "customer-service/index",
          "style": {
            "navigationBarTitleText": "联系客服",
            "disableScroll": true
          }
        },
        {
          "path": "address-form/index",
          "style": {
            "navigationBarTitleText": "新增地址",
            "disableScroll": true
          }
        },
        {
          "path": "progress/index",
          "style": {
            "navigationBarTitleText": "施工进度",
            "disableScroll": true
          }
        },
        {
          "path": "contact-craftsman/index",
          "style": {
            "navigationBarTitleText": "寻找工匠",
            "disableScroll": true
          }
        },
        {
          "path": "message-list/index",
          "style": {
            "navigationBarTitleText": "消息列表",
            "disableScroll": true
          }
        },
        {
          "path": "craftsman-service/index",
          "style": {
            "navigationBarTitleText": "联系工匠",
            "disableScroll": true
          }
        }
      ]
    }
  ],
  "preloadRule": {
    "pages/mine/index": {
      "network": "all",
      "packages": ["subpackages"]
    }
  }
}
